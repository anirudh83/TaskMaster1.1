<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  	<display-name>
      Task Master
    </display-name>
    <servlet>
      <servlet-name>TaskServlet</servlet-name>
      <servlet-class>com.example.web.TaskServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>TaskServlet</servlet-name>
      <url-pattern>*.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    <servlet-name>TaskServlet</servlet-name>
    <url-pattern>/viewTasks</url-pattern>
</servlet-mapping>
</web-app>